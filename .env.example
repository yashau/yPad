# Cloudflare Production Configuration
# Copy this file to .env and fill in your actual values

# Cloudflare Account ID (optional)
# If specified, will be added to wrangler.toml for explicit account targeting
# Example: ACCOUNT_ID=a7f9e2c4d1b6h8j3k5m9p2q4r6s8t0v2
ACCOUNT_ID=

# Worker name for production
WORKER_NAME=ypad

# D1 Database Configuration
DB_NAME=ypad-db
DB_ID=your-database-id-here

# Durable Objects Configuration
DO_SCRIPT_NAME=ypad
